<?xml version="1.0" encoding="ISO-8859-1"?><web-app xmlns="http://java.sun.com/xml/ns/j2ee"    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"    xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd"    version="2.4">  <display-name>Administration Application</display-name>  <description>   administration application.  </description>	<filter>		<filter-name>requestManager</filter-name>		<filter-class>org.cg.dao.webcontainer.tomcat.RequestManagerFilter</filter-class>	</filter>	<filter-mapping>		<filter-name>requestManager</filter-name>		<url-pattern>/*</url-pattern>	</filter-mapping>  <filter>  	<filter-name>authenticationFilter</filter-name>  	<filter-class>org.cg.dao.auth.web.AuthenticationFilter</filter-class>   <init-param>      <param-name>console.user.db.uri</param-name>      <param-value>/WEB-INF/users.list</param-value>    </init-param>       </filter> 	  <filter-mapping>	<filter-name>authenticationFilter</filter-name>	<url-pattern>/console/*</url-pattern>  </filter-mapping>  <filter>  	<filter-name>authorizationFilter</filter-name>  	<filter-class>org.cg.dao.auth.web.AuthorizationFilter</filter-class>   <init-param>      <param-name>allowed.roles</param-name>      <param-value>Administrators,admin</param-value>    </init-param>       </filter> 	  <filter-mapping>	<filter-name>authorizationFilter</filter-name>	<url-pattern>/console/*</url-pattern>  </filter-mapping>   <servlet>    <servlet-name>console</servlet-name>    <servlet-class>      org.cg.dao.console.ExtOsgiManager    </servlet-class>  </servlet>    <servlet-mapping>        <servlet-name>console</servlet-name>        <url-pattern>/console/*</url-pattern>    </servlet-mapping>     <!--     <servlet>    <servlet-name>Bundles</servlet-name>    <servlet-class>      org.apache.felix.webconsole.internal.core.BundlesServlet    </servlet-class>  </servlet>     <servlet-mapping>        <servlet-name>Bundles</servlet-name>        <url-pattern>/console/list/*</url-pattern>    </servlet-mapping>--></web-app>